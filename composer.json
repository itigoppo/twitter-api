{
    "name": "itigoppo/twitter-api",
    "description": "Twitter API Library for PHP",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/oauth-subscriber": "^0.3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "itigoppo",
            "email": "itigoppo+github@gmail.com",
            "homepage": "http://twitter.com/itigoppo"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Itigoppo\\TwitterApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itigoppo\\TwitterApi\\Test\\": "tests/TestCase"
        }
    }
}
